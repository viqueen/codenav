  },
  "homepage": "https://github.com/viqueen/codenav#readme",
  "devDependencies": {
    "@types/node": "^14.14.31",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty:check"
    }
  },
  "dependencies": {
    "commander": "^6.0.0",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
